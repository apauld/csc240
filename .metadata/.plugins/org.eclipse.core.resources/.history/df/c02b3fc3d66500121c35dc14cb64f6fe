
public class MyStructuredRunner {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		String firstName;
		String lastName;
		String address;
		String city;
		String state;
		int zip;
		
		talk("Hello structured world!");
		
	}
	
	public static void speak(String talk){
		System.out.println(talk);
	}

}
